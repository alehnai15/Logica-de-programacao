programa {
  funcao inicio() {
    inteiro idade 

    se (idade >= 65) {
      escreva("Você é um idoso")
    }
    senao se (idade >= 18){
      escreva("Você é um adulto")
    }
    senao se (idade >= 13){
      escreva("Você é um adolecente")
    }
    senao se (idade <= 12){
      escreva("Você é uma criança")
    }
    
  }
}
